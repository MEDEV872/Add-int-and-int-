using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp4
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = 14;
            int b = 28;
            Console.WriteLine("The value of a + b is : " + (a + 
                b));
            Console.WriteLine("The value of a + b is : " + (a -
                b));
            Console.WriteLine("The value of a * b is : " + (a  *
               b));
            Console.WriteLine("The value of a + b is : " + (a  /
                b));
            Console.ReadLine();
        }
    }
}
